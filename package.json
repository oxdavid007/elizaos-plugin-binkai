        "cli-table3": "^0.6.3",
        "ethers": "^6.13.5",
        "mime-types": "^2.1.35",
        "openai": "^4.85.2",
        "ora": "^8.0.1",
        "solc": "^0.8.28",
        "tsup": "8.3.5",
        "viem": "2.21.53",
        "ws": "^8.18.0",
        "zod": "^3.24.3",
        "uuid": "^11.1.0"
    },
    "devDependencies": {
        "@biomejs/biome": "1.9.4",
        "vitest": "^3.1.2"
    },
    "scripts": {
        "build": "tsup --format esm --dts",
        "dev": "tsup --format esm --dts --watch",
        "test": "vitest run",
        "clean": "rm -rf dist",
        "lint": "biome lint .",
        "lint:fix": "biome check --apply .",
        "format": "biome format .",
        "format:fix": "biome format --write ."
    },
    "peerDependencies": {
        "@types/mime-types": "^2.1.4",
        "whatwg-url": "7.1.0"
    },
    "agentConfig": {
        "pluginType": "elizaos:client:1.0.0",
        "pluginParameters": {
            "BNB_SEED_PHRASE": {
                "type": "string",
                "minLength": 1,
                "description": "Your seed phrase for the BNB Chain ecosystem. Grants full access to associated funds.",
                "optional": true
            },
            "BSC_RPC_URL": {
                "type": "string",
                "minLength": 1,
                "description": "Custom RPC URL for BSC.",
                "optional": true
            },
            "ETHEREUM_RPC_URL": {
                "type": "string",
                "minLength": 1,
                "description": "Custom RPC URL for Ethereum.",
                "optional": true
            },
            "SOLANA_RPC_URL": {
                "type": "string",
                "minLength": 1,
                "description": "Custom RPC URL for Solana.",
                "optional": true
            },
            "BIRDEYE_API_KEY": {
                "type": "string",
                "minLength": 1,
                "description": "Your API key for Birdeye.",
                "optional": true
            },
            "ALCHEMY_API_KEY": {
                "type": "string",
                "minLength": 1,
                "description": "Your API key for Alchemy.",
                "optional": true
            },
            "OPENAI_API_KEY": {
                "type": "string",
                "minLength": 1,
                "description": "Your API key for OpenAI.",
                "optional": true
            },
            "BINK_API_KEY": {
                "type": "string",
                "minLength": 1,
                "description": "Your API key for Bink.",
                "optional": true
            },
            "BINK_BASE_URL": {
                "type": "string",
                "minLength": 1,
                "description": "Your base URL for Bink.",
                "optional": true
            },
            "BINK_IMAGE_API_URL": {
                "type": "string",
                "minLength": 1,
                "description": "Your image API URL for Bink.",
                "optional": true
            },
            "BINK_IMAGE_API_KEY": {
                "type": "string",
                "minLength": 1,
                "description": "Your image API key for Bink.",
                "optional": true
            }
        }
    }
}